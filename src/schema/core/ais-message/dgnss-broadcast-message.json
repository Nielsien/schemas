{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://poseidat.org/schema/core/ais-message/dgnss-broadcast-message.json",
  "title": "DGNSS corrections provided by a base station",
  "description": "Global navigation satellite system corrections provided by a base station",
  "properties": {
    "ais_message_type": {
      "description": "The message type of the given AIS measurement",
      "type": "string",
      "enum": [
        "DGNSS_BROADCAST_MESSAGE"
      ],
      "default": "DGNSS_BROADCAST_MESSAGE"
    },
    "position": {
      "description": "The position of the AIS entity",
      "$ref": "https://poseidat.org/schema/core/measurement/position.json"
    },
    "data": {
      "description": "DGNSS correction data in text",
      "type": "string"
    }
  },
  "required": ["ais_message_type", "position", "data"],
  "allOf": [
    { "$ref": "https://poseidat.org/schema/core/ais-message/base-ais-message.json" }
  ],
  "type": "object",
  "additionalProperties": false
}

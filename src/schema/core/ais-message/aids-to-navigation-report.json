{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://poseidat.org/schema/core/ais-message/aids-to-navigation report.json",
  "title": "Aids to navigation report AIS",
  "description": "Position and status report for aids-to-navigation",
  "properties": {
    "ais_message_type": {
      "description": "The message type of the given AIS measurement",
      "type": "string",
      "enum": [
        "AIDS_TO_NAVIGATION_REPORT"
      ],
      "default": "AIDS_TO_NAVIGATION_REPORT"
    },
    "entity_name": {
      "description": "The name of the AIS entity",
      "type": "string",
      "maximum": 20,
      "default": "@@@@@@@@@@@@@@@@@@@@"
    },
    "position": {
      "description": "The position of the AIS entity",
      "$ref": "https://poseidat.org/schema/core/measurement/position.json"
    },
    "position_accuracy": {
      "description": "The accuracy of the given AIS position. LOW is > 10 meters, HIGH is <= 10 meters ",
      "$ref": "https://poseidat.org/schema/enum/ais-position-accuracy.json",
      "default": "LOW"
    },
    "aid_type": {
      "description": "The type of aid from this entity",
      "$ref": "https://poseidat.org/schema/enum/ais-aid-type.json",
      "default": "NOT_AVAILABLE"
    },
    "aid_name_extension": {
      "description": "Extends the aids to navigation name of the entity",
      "type": "string"
    },
    "aton_status": {
      "description": "indication of the AtoN status",
      "type": "string",
      "default": "00000000"
    },
    "off_position_indicator": {
      "description": "Indicates if the AtoN is on or off position",
      "enum":  [
        "ON_POSITION",
        "OFF_POSITION"
      ],
      "type": "string",
     "default": "ON_POSITION"
    },
    "virtual_aton_flag": {
      "description": "indicates if the AtoN physically exists or if it is simulated",
      "enum": [
        "REAL_ATON",
        "VIRTUAL_ATON"
      ],
      "type": "string",
      "default": "REAL_ATON"
    },
    "dimension_to_bow": {
      "description": "Indicates the dimension of ship",
      "type": "integer",
      "default": 0
    },
    "dimension_to_stern": {
      "description": "Indicates the dimension of ship",
      "type": "integer",
      "default": 0
    },
    "dimension_to_port": {
      "description": "Indicates the dimension of ship",
      "type": "integer",
      "default": 0
    },
    "dimension_to_starboard": {
      "description": "Indicates the dimension of ship",
      "type": "integer",
      "default": 0
    },
    "position_device_type": {
      "description": "The type of navigation device",
      "$ref": "https://poseidat.org/schema/enum/ais-position-device-type.json",
      "default": "SURVEYED"
    },
    "raim_flag": {
      "description": "Receiver autonomous integrity monitoring (RAIM) flag of electronic position fixing device",
      "$ref": "https://poseidat.org/schema/enum/ais-raim-flag.json",
      "default": "RAIM_NOT_IN_USE"
    },
    "assigned_mode_flag": {
      "description": "State of station if it is operating in autonomous or assigned mode",
      "$ref": "https://poseidat.org/schema/enum/ais-assigned-mode-flag.json",
      "default": "AUTONOMOUS_CONTINUOUS_MODE"
    }
  },
  "required": [ "ais_message_type", "entity_name", "position", "position_accuracy", "aid_type" , "aid_name_extension",
    "aton_status", "off_position_indicator", "virtual_aton_flag", "dimension_to_bow", "dimension_to_stern", "dimension_to_port",
  "dimension_to_starboard", "position_device_type", "raim_flag", "assigned_mode_flag"],
  "allOf": [
    { "$ref": "https://poseidat.org/schema/core/ais-message/base-ais-message.json" }
  ],
  "type": "object",
  "additionalProperties": false
}
